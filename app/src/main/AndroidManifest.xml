<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.retalayman.coursecenterapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/eclogo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterationActivity" />
        <activity android:name=".MainListActivity" />
        <activity android:name=".MainDetailsActivity" />
        <activity
            android:name=".Main2MenueActivity"
            android:label="@string/title_activity_main2_menue"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>